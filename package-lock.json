{
  "name": "yarnworkspace",
  "version": "1.0.0",
  "lockfileVersion": 3,
  "requires": true,
  "packages": {
    "": {
      "name": "yarnworkspace",
      "version": "1.0.0",
      "license": "MIT",
      "workspaces": [
        "packages/*"
      ],
      "dependencies": {
        "pnpm": "^8.9.0"
      },
      "engines": {
        "node": "v18.16.0",
        "yarn": "1.22.19"
      }
    },
    "node_modules/@pkg/demo": {
      "resolved": "packages/demo",
      "link": true
    },
    "node_modules/@pkg/demo2": {
      "resolved": "packages/demo2",
      "link": true
    },
    "node_modules/pnpm": {
      "version": "8.9.0",
      "resolved": "https://registry.npmjs.org/pnpm/-/pnpm-8.9.0.tgz",
      "integrity": "sha512-74hZk44fBTe5/PAwkEQxE5Lzs4s0QXbmzU/e4hsiVSSwrCobCK4q4t3Vs/9LjKSW1neOlQ8+fJ9VW4EyWYJEHA==",
      "bin": {
        "pnpm": "bin/pnpm.cjs",
        "pnpx": "bin/pnpx.cjs"
      },
      "engines": {
        "node": ">=16.14"
      },
      "funding": {
        "url": "https://opencollective.com/pnpm"
      }
    },
    "packages/demo": {
      "name": "@pkg/demo",
      "version": "1.0.0",
      "license": "MIT"
    },
    "packages/demo2": {
      "name": "@pkg/demo2",
      "version": "1.0.0",
      "license": "MIT",
      "dependencies": {
        "@pkg/demo": "1.0.0"
      }
    }
  }
}
